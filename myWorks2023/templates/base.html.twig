							<a class="nav-link" href="{{ path('app_hobbies_index') }}">Hobbies</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="{{ path('app_languages_index') }}">Languages</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="{{ path('app_sites_index') }}">Sites</a>
						</li>
						<li class="nav-item">
							<a class="nav-link" href="{{ path('app_skills_index') }}">Skills</a>
						</li>
						{# <li class="nav-item">
							<a class="nav-link disabled" href="{{ path('app_categories_index') }}">Catégories</a>
						</li> #}
					</ul>
					<form class="form-inline my-2 my-lg-0">
						<input class="form-control mr-sm-2" type="search" placeholder="Search" arialabel="Search">
						<button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
					</form>
				</div>
			</nav>
			{% block body %}{% endblock %}
		</body>
    <script src="https://kit.fontawesome.com/50b4ed0339.js" crossorigin="anonymous"></script>
	</html>
